/* pages/usualContact/usualContact.wxss */
page{
  background: #efefef;
  font-size: 28rpx;
  line-height: 40rpx;
}
.tit{
  padding: 20rpx;
  background: #fff;
}
.tit navigator{
  font-size: 50rpx;
  color: #2b9a26;
  border: 1rpx solid #2b9a26;
  border-radius: 50%;
  height: 40rpx;
  width: 40rpx;
  line-height: 40rpx;
  text-align: center;
}
.list{
  padding-left: 30rpx;
  border-top: 1rpx solid #ababab;
  border-bottom: 1rpx solid #ababab;
  background: #fff;
}
.list .checkbox{
  padding: 30rpx 0;
  border-bottom: 1rpx solid #dddddd;
}
.list .checkbox:last-child{
  border-bottom: none;
}
.list .info{
  width: 620rpx;
}
.list text{
  color: #767676;
}
button{
  background: #f13838;
  width: 620rpx;
  color: #fff;
  margin: 60rpx auto;
}