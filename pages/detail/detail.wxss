.group{
  position: fixed;
  width: 750rpx;
  height: 100%;
  top: 0;
  left: 0;
  background: rgba(0,0,0,.6);
  z-index: 500;
}
.group-meng{
  position: fixed;
  width: 750rpx;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 0;
}
.group .swiper{
  height: 100%;
}
.group .group-box{
  width: 542rpx;
  height: 800rpx;
  margin: 132rpx 104rpx;
  position: relative;
  background: #fff;
  border-radius: 20rpx;
  overflow: auto;
  background-repeat: no-repeat; 
}
.group .group-box::-webkit-scrollbar {
	display:none;
}
.group-box .info image{
  width: 26rpx;
  height: 26rpx;
  margin-right: 16rpx;
}
.group-box .name{
  padding: 48rpx 0 16rpx 0;
  text-align: center;
  font-size: 40rpx;
}
.group-box .info{
  font-size: 26rpx;
  padding: 8rpx;
  margin-left: 116rpx;
  display: flex;
  align-items: center;
}
.group-box .info .score{
  width: 136rpx;
  height: 24rpx;
}
.group-box .head{
  position: absolute;
  top: 284rpx;
  left: 208rpx;
  width: 126rpx;
  height: 126rpx;
  border-radius: 50%;
  box-shadow: 0 10rpx 20rpx rgba(42,186,110,.6);
}
.group-box .sex{
  width: 26rpx;
  height: 26rpx;
}
.group-box .bg{
  color: #fff;
  background: #2aba6e;
  height: 346rpx;
}
.group-box .name-main{
  font-size: 28rpx;
  margin: 80rpx 0 24rpx 0;
  text-align: center;
}
.group-box .decribe{
  padding: 0 26rpx;
  font-size: 24rpx;
  color: #616266;
  line-height: 36rpx;
  text-indent: 2em;
}
.meng-text{
  position: fixed;
  bottom: 272rpx;
  width: 542rpx;
  height: 100rpx;
  background: -webkit-repeating-linear-gradient(bottom, #fff 0%, rgba(255, 255,255 , 0) 100%);
  border-bottom-right-radius: 20rpx;
  border-bottom-left-radius: 20rpx;
}
.seal{
  width: 180rpx;
  height: 200rpx;
  position: fixed;
  right: 118rpx;
  bottom: 288rpx;
}
.consult-box{
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  margin: auto;
  width: 500rpx;
  height: 346rpx;
  background: #fff;
  border-radius: 20rpx;
  z-index: 100;
  text-align: center;
}
.consult-box .tit{
  line-height: 100rpx;
  font-size: 36rpx;
  border-bottom: 1rpx solid #eee;
}
.consult-box .contact{
  width: 100%;
  margin-top: 10rpx;
  line-height: 60rpx;
  font-size: 30rpx;
  color: #606266;
}
.consult-box image{
  width: 160rpx;
  height: 160rpx;
}