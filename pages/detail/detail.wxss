/* pages/detail/detail.wxss */
.carousel-box{
  height: 320rpx;
  position: relative;
}
.carousel-img{
  width: 750rpx;
  height: 320rpx;
}
.carousel-box text{
  position: absolute;
  bottom: 0;
  right: 0;
  color: #fff;
  padding: 6rpx;
  font-size: 24rpx;
  background: #e11a2c;
}
.tab{
  padding: 0 20rpx;
  border-bottom: 1rpx solid #efeff4;
}
.tab view{
  font-size: 28rpx;
  color: #606266;
  padding: 30rpx 0;
  width: 140rpx;
  text-align: center;
}
.tab .active{
  border-bottom: 4rpx solid #2aba6e;
  color: #2aba6e;
}
.intro .act-name{
  padding: 20rpx;
  line-height: 60rpx;
  font-size: 36rpx;
  color: #000;
}
.intro .price{
  display: flex;
  flex-direction: row;
  padding: 0 0 20rpx 30rpx;
  align-items: center;
}
.intro .price image{
  width: 40rpx;
  height: 40rpx;
}
.intro .price .num{
  color: #f43531;
  font-size: 28rpx;
  margin: 0 10rpx;
}
.minus,.first,.discount{
  color: #fff;
  padding: 4rpx;
  border-radius: 4rpx;
  font-size: 24rpx;
  margin: 0 6rpx;
}
.minus{
  background: #ff635b;
}
.first{
  background: #ffa628;
}
.discount{
  background: #c183e2;
}
.location{
  height: 82rpx;
  padding: 0 30rpx;
}
.location view{
  font-size: 28rpx;
  width: 580rpx;
}
.location image{
  width: 40rpx;
  height: 42rpx;
}
.time{
  padding: 10rpx 30rpx;
}
.time image{
  width: 40rpx;
  height: 40rpx;
  vertical-align: middle;
}
.time .tit{
  font-size: 28rpx;
  vertical-align: middle;  
}
.time .tit text{
  padding-left: 20rpx;
}
.times text{
  display: inline-block;
  font-size: 24rpx;
  padding: 14rpx;
  border: 1rpx solid #909399;
  color: #909399;
  border-radius: 8rpx;
  margin: 14rpx;
}
.times .active{
  color: #55c88b;
  border-color: #2aba6e;
}
.join{
  padding: 10rpx 0;
}
.join image{
  width: 40rpx;
  height: 40rpx;
  vertical-align: middle;
}
.join .tit{
  margin: 0 30rpx;
  font-size: 28rpx;
  vertical-align: middle;  
}
.join .people-box{
  width: 750rpx;
  overflow: auto;
}
.join .people-box::-webkit-scrollbar {
  display:none;
}
.join .people{
  width: 1000rpx;
}
.join .people view{
  display: inline-block;
  width: 148rpx;
  text-align: center;
  padding-top: 20rpx;
}
.join .people text{
  display: block;
  font-size: 24rpx;
  margin-top: 10rpx;
}
.join .people image{
  height: 80rpx;
  width: 80rpx;
  border-radius: 50%;
}
.join .total{
  width: 500rpx;
  text-align: right;
  color: #c7c7cc;
}
.join .total text{
  color: #2aba6e;
}
.join .alreadynum{
  color: #2aba6e;
}
.same,.act-detail{
  font-size: 28rpx;
  padding: 30rpx;
  color: #4f81bd;
}
.same text{
  display: block;
  color: #f00;
  font-size: 32rpx;
}
.big-spacing{
  height: 40rpx;
  background: #efeff4;
}
.consult{
  position: fixed;
  right: 10rpx;
}
.consult image{
  width: 132rpx;
  height: 132rpx;
}
.detail-nav{
  height: 100rpx;
  position: fixed;
  width: 750rpx;
  bottom: 0;
}
.detail-nav image{
  height: 46rpx;
  width: 52rpx;
  margin-top: 10rpx;
}
.detail-nav .sign{
  width: 528rpx;
  text-align: center;
  color: #fff;
  background: #2aba6e;
  line-height: 100rpx;
  height: 100rpx;
  font-size: 34rpx;
  border-top: 1rpx solid #2aba6e;
}
.detail-nav text{
  display: block;
}
.detail-nav .border-top{
  border-top: 1rpx solid #d9d9d9;
}
.detail-nav .border-left{
  border-left: 1rpx solid #d9d9d9;
}
.detail-nav navigator,.detail-nav view{
  width: 110rpx;
  height: 100rpx;
  background: #fff;
  text-align: center;
  font-size: 22rpx;
}
.follow{
  position: fixed;
  width: 690rpx;
  height: 100rpx;
  padding: 0 30rpx;
  bottom: 100rpx;
  font-size: 24rpx;
  background: rgba(0,0,0,.6);
  color: #fff;
}
.follow .close{
  width: 26rpx;
  height: 26rpx;
}
.follow .logo{
  width: 74rpx;
  height: 74rpx;
}
.follow .payattention{
  padding: 10rpx;
  border-radius: 10rpx;
  background: #f37f29;
}
.follow .describe{
  width: 220rpx;
  margin-right: 200rpx;
}
.qr-code{
  position: fixed;
  margin: auto;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  width: 588rpx;
  height: 654rpx;
  text-align: center;
  background-image: url("data:image/png;base64,");
  background-size: 588rpx 654rpx;
  z-index: 100;
}
.qr-code image{
  width: 284rpx;
  height: 284rpx;
  /* margin-bottom: 24rpx; */
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  z-index: 102;
}
