.safe{
  font-size: 24rpx;
  line-height: 80rpx;
  padding: 0 30rpx;
  background: #fff;
}
.safe text{
  color: #ff5047;
}
.total-fee{
  line-height: 100rpx;
  padding-left: 30rpx;
  font-size: 28rpx;
  background: #fff;
  border-bottom: 1rpx solid #d9d9d9;
}
.total-fee .pay{
  background: #2aba6e;
  color: #fff;
  padding: 0 50rpx;
}
.safe-box{
  width: 550rpx;
  height: 720rpx;
  padding: 30rpx;
  border-radius: 30rpx;
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  background: #fff;
  z-index: 999;
}
.safe-box .tit{
  text-align: center;
  font-size: 28rpx;
  font-weight: 900;
  margin-bottom: 30rpx;
}
.safe-box text{
  display: block;
  font-size: 24rpx;
  text-indent: 1em;
  line-height: 30rpx;
}
.safe-box button{
  /* margin-top: 50rpx; */
  position: absolute;
  bottom: 30rpx;
  font-size: 24rpx;
  width: 550rpx;
}