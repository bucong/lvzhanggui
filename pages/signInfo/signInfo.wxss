/* pages/signInfo/signInfo.wxss */
page{
  background: #efeff4;
}
.act-name{
  padding:  20rpx 40rpx;
  font-size: 36rpx;
  color: #2aba6e;
  line-height: 60rpx;
  background: #fff;
}
.money{
  padding: 24rpx 32rpx;
  font-size: 28rpx;
  background: #fff;
}
.money .num{
  color: #e11a2c;
}
.time{
  background: #fff;
}
.time .tit{
  font-size: 28rpx;
  font-weight: 900;
  padding: 20rpx 0;
  margin-left: 30rpx;
  vertical-align: middle;  
  border-bottom: 1rpx solid #eee;
  margin-bottom: 20rpx;
}
.addresses{
  padding: 10rpx 0 10rpx 0;
  overflow: auto;
}.addresses::-webkit-scrollbar{
  display: none;
}
.address-box{
  padding-left: 30rpx;
  padding-bottom: 20rpx;
  font-size: 24rpx;
}
.address-box .address{
  display: inline-block;
  width: 180rpx;
  height: 58rpx;
  padding: 30rpx 60rpx 40rpx 20rpx;
  vertical-align: middle;
  background-repeat: no-repeat !important;
}
.address-box .address{
  /* background: url(../images/add3.png); */
  background: url("data:image/png;base64,");
  background-size: 260rpx 118rpx;
}
.address-box .address:first-child{
  /* background: url(../images/add1.png); */
  background: url("data:image/png;base64,");
  background-size: 260rpx 118rpx;
} 
.address-box .address:last-child{
  /* background: url(../images/add5.png); */
  background: url("data:image/png;base64,");
  background-size: 260rpx 118rpx;
}
.addresses .address-box .active{
  /* background: url(../images/add4.png); */
  background: url("data:image/png;base64,");
  background-size: 260rpx 118rpx;
}
.address-box .address:first-child.active{
  /* background: url(../images/add2.png); */
  background: url("data:image/png;base64,");
  background-size: 260rpx 118rpx;
} 
.address-box .address:last-child.active{
  /* background: url(../images/add6.png); */
  background: url("data:image/png;base64,");
  background-size: 260rpx 118rpx;
} 
.people-box{
  background: #fff;
}
.people{
  width: 720rpx;
  overflow: hidden;
  height: 80rpx;
  line-height: 78rpx;
  padding-left: 30rpx;
  position: relative;
}
.people-box .people .active{
  left: -100rpx;
}
.people::-webkit-scrollbar {
  display:none;
}
.people .flex{
  position: absolute;
  width: 850rpx;
  border-bottom: 1rpx solid #ccc;
  padding-right: 30rpx;
  left: 30rpx;
  transition: .5s;
  font-size: 26rpx;
}
.people .del-btn{
  width: 120rpx;
  color: #fff;
  background: #ff3b30;
  text-align: center;
}
.people-box .add-people{
  justify-content: center;
}
.people-box .add-people image{
  width: 40rpx;
  height: 40rpx;
  margin-right: 20rpx;
}
.people .del{
  width: 32rpx;
  text-align: center;
  line-height: 32rpx;
  border-radius: 50%;
  background: #ff3b30;
  color: #fff;
}
.people-box .add-people{
  line-height: 80rpx;
  color: #2aba6e;
}
.cards-box{
  color: #333;
  font-size: 28rpx;
  background: #fff;
}
.card-list{
  height: 82rpx;
  margin: 0 20rpx;
  border-top: 1rpx solid #ddd;
}
.card-list text{
  color: #777;
}
.card-list:first-child{
  border-top: none;
}
