/* pages/people/people.wxss */
page{
  background: #efeff4;
}
.people{
  background: #3dbf7b;
  color: #fff;
  padding-left: 46rpx;
  border-bottom: 1rpx solid #c1e7d1;
}
.people .info{
  align-items: flex-start;
  border-bottom: 1rpx solid #c1e7d1;
  padding: 24rpx 24rpx 24rpx 0;
}
.people .info-head{
  width: 110rpx;
  height: 110rpx;
  border-radius: 50%;
  border: 6rpx solid #fff;
}
.people .flex .name{
  font-size: 36rpx;
  font-weight: 900;
  margin-bottom: 10rpx;
}
.people .sex{
  width: 26rpx;
  height: 26rpx;
}
.people .sign{
  color: #c1e7d1;
  font-size: 24rpx;
}
.opertion-icon{
  width: 50rpx;
  height: 50rpx;
  margin-bottom: 30rpx;
}
.people .own{
  font-size: 32rpx;
  padding: 24rpx 50rpx 24rpx 0;
  font-weight: 900;
}
.people .own text{
  display: block;
}
.people .own .authority{
  width: 172rpx;
  height: 84rpx;
  border-radius: 12rpx;
  text-align: center;
  line-height: 84rpx;
}
.people-tab{
  background: #3dbf7b;
  justify-content: space-around;
  height: 170rpx;
}
.people-tab view{
  text-align: center;
  width: 187rpx;
  height: 140rpx;
  padding-top: 30rpx;
}
.people-tab image{
  width: 52rpx;
  height: 46rpx;
}
.people-tab text{
  display: block;
  color: #fff;
  font-size: 24rpx;
}
.people-tab .active{
  font-weight: 900;
  background: url("data:image/jpeg;base64,");
  background-size: 187rpx 170rpx;
}
.unloginContent image{
  display: block;
  margin: 80rpx auto;
}
.contant{
  padding: 20rpx 15rpx;
}
.tab-contant{
  display: none;
  font-size: 24rpx;
}
.contant .active{
  display: block;
}
/* 卡券页面 */
.cards-box{
  background: #fff;
  padding: 16rpx;
  border-radius: 10rpx;
  overflow: hidden;
}
.cards-box .cards-manage view{
  width: 380rpx;
}
.cards-box .cards-manage navigator{
  width: 84rpx;
  height: 40rpx;
  background: #f37f29;
  border-radius: 10rpx;
  color: #fff;
  font-size: 22rpx;
  text-align: center;
  line-height: 40rpx;
}
.cards-box .icon{
  width: 48rpx;
  height: 32rpx;
}
.cards-box .up-down{
  width: 40rpx;
  height: 40rpx;
}
.cards-box .cards-list{
  width: 690rpx;
  height: 240rpx;
  position: relative;
  margin-top: 12rpx;
}
.cards-box .cards-list image{
  position: absolute;
  top: 0;
  left: 0;
  width: 690rpx;
  height: 180rpx;
  z-index: 0;
}
.cards-box .cards-list .cards-name{
  position: absolute;
  top: 60rpx;
  left: 260rpx;
  font-size: 40rpx;
  color: #fff;
  z-index: 10;
}
.cards-box .cards-list .cards-info{
  position: absolute;
  bottom: 16rpx;
  left: 20rpx;
  width: 650rpx;
  color: #666;
  z-index: 10;
}

/* 权益页面 */
.power{
  background: #fff;
}
.power .active{
  height: auto;
}
.power-list{
  overflow: hidden;
  border-bottom: 1rpx solid #ddd;
  height: 88rpx;
}
.power-list .name{
  height: 88rpx;
  line-height: 88rpx;
  font-size: 34rpx;
  color: #777;
  text-align: center;
}
.power-list .detail{
  border-top: 8rpx solid #199013;
  font-size: 24rpx;
  padding: 20rpx 0 40rpx 40rpx;
  color: #777;
  line-height: 44rpx;
}
.power-list .detail .tit{
  color: #333;
  font-size: 30rpx;
}
/* 活动页面 */
.activity-list{
  height: 340rpx;
}
.activity-list .left,.activity-list .right {
  float: left;
  height: 300rpx;
  padding-top: 40rpx;
}
.activity-list::after{
  content: '';
  display: block;
  clear: both;
}
.activity-list .left{
  width: 160rpx;
  text-align: right;
  padding-right: 20rpx;
  position: relative;
  overflow: visible;
}
.activity-list .left text{
  position: absolute;
  top: 42rpx;
  right: -16rpx; 
  display: inline-block; 
  z-index: 999;
  width: 28rpx;
  height: 28rpx;
  border: 1rpx solid #999;
  background: #eee;
  border-radius: 50%;
}
.activity-list .right{
   border-left: 1rpx solid #999; 
  padding-left: 32rpx;
}
.activity-list .right view{
  font-size: 28rpx;
  margin-bottom: 20rpx;
}
.activity-list .right text{
  display: block;
  margin-top: 32rpx;
  border-top: 1rpx solid #999;
}
.activity-list .image{
  width: 500rpx;
  height: 200rpx;
}

/* 联系人 */
.contact{
  background: #fff;
  border-radius: 10rpx;
}
.contactTit{
  padding: 20rpx;
  font-size: 30rpx;
  border-bottom: 1rpx solid #aaa;
}
.contactTit navigator image{
  height: 46rpx;
  width: 46rpx;
}
.contact-list:last-child{
  border-bottom: none;
}
.contact-list{
  margin-left: 20rpx;
  padding: 20rpx 0;
  border-bottom: 1rpx solid #ddd;
}
.contact-list .del{
  height: 56rpx;
  width: 56rpx;
  margin-right: 40rpx;
}